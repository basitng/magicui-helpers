{
  "name": "magicui-react-hooks",
  "version": "1.0.5",
  "description": "A collection of helpful hooks for React",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --outDir dist",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "prettier": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "keywords": [
    "react",
    "utilities",
    "hooks",
    "helpers",
    "typescript"
  ],
  "author": "(Ajaga Abdulbasit) basitng2004@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/basitng/magicui-hooks.git"
  },
  "bugs": {
    "url": "https://github.com/basitng/magicui-hooks/issues"
  },
  "homepage": "https://github.com/basitng/magicui-hooks#readme",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@swc/core": "^1.4.17",
    "@types/jest": "^29.5.12",
    "@types/ms": "^0.7.34",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.3.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
